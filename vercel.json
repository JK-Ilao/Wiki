{
    "version": 2,
    "builds": [{ "src": "./server/server.js", "use": "@vercel/node" }],
    "routes": [
        {
            "src": "/(.*)",
            "destination": "./server/server.js"
        }
    ],
    "rewrites": [
        {
            "src": "/(.*)",
            "destination": "./server/server.js"
        }
    ]
}